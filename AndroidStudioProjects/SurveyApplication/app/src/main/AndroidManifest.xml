        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".firstqs" />
        <activity android:name=".firstsurvey" />
        <activity android:name=".secondservey" />
        <activity android:name=".thirdServey" />
        <activity android:name=".forthServey" />
        <activity android:name=".fifthSurvey" />
        <activity android:name=".lastsurvey" />
        <activity android:name=".Dashboard"></activity>
    </application>

</manifest>